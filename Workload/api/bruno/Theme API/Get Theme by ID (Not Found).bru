meta {
  name: Get Theme by ID (Not Found)
  type: http
  seq: 3
}

get {
  url: {{base_url}}/themes/{{non_existent_theme_id}}
  body: none
  auth: none
}

headers {
  Content-Type: application/json
  x-ms-client-principal: {{auth_header}}
