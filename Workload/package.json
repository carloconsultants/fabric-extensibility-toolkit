{
  "name": "fabric-developer-sample",
  "version": "2.0.0",
  "description": "Microsoft Fabric - Developer Sample App",
  "scripts": {
    "start": "npm run start:devServer",
    "start:devServer": "env-cmd -f .env.dev vite",
    "start:apiServer": "node ./devServer/api-server.js",
    "start:devGateway": "node ./devServer/start-devGateway.js",
    "start:codespace": "env-cmd -f .env.dev vite --host",
    "local": "npm run build:manifest:local && env-cmd -f .env.dev vite --port 60000",
    "swa": "env-cmd -f .env.dev swa start http://localhost:60000 --api-devserver-url http://localhost:7071 --host 0.0.0.0 --port 60006",
    "build:test": "env-cmd -f .env.test vite build --outDir ../build/Frontend",
    "build:prod": "env-cmd -f .env.prod vite build --outDir ../build/Frontend --mode production",
    "build:manifest:local": "cd ../scripts/Build && pwsh ./BuildManifestPackage.ps1 -Environment dev",
    "build:manifest:prod": "cd ../scripts/Build && pwsh ./BuildManifestPackage.ps1 -Environment prod",
    "preview": "vite preview"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@carloconsultants/cs-ui-library": "^1.0.0",
    "@fluentui/react": "^8.110.7",
    "@fluentui/react-components": "^9.7.2",
    "@fluentui/react-icons": "2.0.226",
    "@ms-fabric/workload-client": "^3.0.0",
    "@reduxjs/toolkit": "^2.6.0",
    "@tanstack/react-query": "^5.0.0",
    "history": "^4.9.0",
    "i18next": "^25.4.1",
    "i18next-http-backend": "^2.4.2",
    "jwt-decode": "^3.1.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^15.0.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^5.3.4",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@vitejs/plugin-react": "^4.3.1",
    "ajv": "^8.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "nuget-bin": "^4.0.0",
    "sass": "^1.79.3",
    "typescript": "^5.0.4",
    "vite": "^7.1.7",
    "vite-plugin-env-compatible": "^2.0.1"
  }
}
